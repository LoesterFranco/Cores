    130 0000E002 E0 D2                   		sw		r2,[sp]
    131 0000E004 00 40 0F 12             		lw		r2,#15
    132 0000E008                        zeroTxBuf1:
    133 0000E008 02 40 20 C0             		sb		r0,txBuf[r2]
