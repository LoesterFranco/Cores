                                        ; Registers Affected:
                                        ;	<none>
                                        ;----------------------------------------------------------------------------
                                        
    158 0000E024 FC 6E                  Xmit:	
    159 0000E026 E0 D1                   		sw		r1,[sp]
    160 0000E028 E2 D2                   		sw		r2,2[sp]
                                        		; wait for transmit buffer to empty
    162 0000E02A                        Xmit2:
    163 0000E02A 01 4B 02 E1             		lb		r1,ROUTER+RTR_TXSTAT
    164 0000E02E FD B9                   		bne		Xmit2
