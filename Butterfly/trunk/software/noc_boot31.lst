                                        ;----------------------------------------------------------------------------
                                        ; Receive from network.
                                        ; Receive status must have already indicated a message present.
                                        ; Copies recieve buffer from router to rxBuf.
                                        ; The router fifo is configured with first word fall-through. This means
                                        ; the data word appears on the fifo output before the fifo is popped.
                                        ; A fifo pop signal has to be sent after reading the data.
                                        ;
                                        ; Parameters:
                                        ;	<none>
                                        ; Returns:
                                        ;	<none>
                                        ; Registers Affected:
                                        ;	<none>
                                        ;----------------------------------------------------------------------------
                                        
    199 0000E04E                        Recv:
    200 0000E04E FC 6E                   		add		sp,sp,#-4
    201 0000E050 E0 D1                   		sw		r1,[sp]
    202 0000E052 E2 D2                   		sw		r2,2[sp]
    203 0000E054 00 40 0F 12             		lw		r2,#15
    204 0000E058                        Recv1:
    205 0000E058 00 4B 20 E1             		lb		r1,ROUTER[r2]			; copy message to local buffer
    206 0000E05C 03 40 20 C1             		sb		r1,rxBuf[r2]
    207 0000E060 FF 62                   		add		r2,r2,#-1
    208 0000E062 FA BB                   		bpl		Recv1
    209 0000E064 01 4B 00 E1             		lb		r1,ROUTER+RTR_RXSTAT
    210 0000E068 04 40 60 31             		or		r1,#$40
    211 0000E06C 01 4B 00 C1             		sb		r1,ROUTER+RTR_RXSTAT	; pop the rx fifo
    212 0000E070 E0 F1                   		lw		r1,[sp]
    213 0000E072 E2 F2                   		lw		r2,2[sp]
    214 0000E074 04 6E                   		add		sp,sp,#4
    215 0000E076 F0 80                   		ret
                                        
