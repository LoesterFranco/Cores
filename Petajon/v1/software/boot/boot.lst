                        	;------------------------------------------------------------------------------
                        	; Insert task into ready list. The list is a doubly linked circular list.
                        	;
                        	; Parameters:
                        	;		a0 = tid to insert
                        	; Modifies:
                        	;		v1,t0,t1,t2
                        	; Returns:
                        	;		v0 = 1 for success, 0 if failed
                        	;------------------------------------------------------------------------------
                        	
                        	InsertIntoReadyList:
FFFC2424 00006813		ldi		$v0,#E_Ok						; setup for ok
FFFC2428 00F06893		ldi		$v1,#MAX_TID				; check argument
FFFC242C 0928EA63		bgtu	$a0,$v1,.badTid
FFFC2430 00A91E93		sll		$t3,$a0,#LOG_TCBSZ	; tid to pointer
FFFC2434 288ECD03		ldbu	$t0,TCBStatus[$t3]	; set status to ready
FFFC2438 001D6D13		or		$t0,$t0,#TS_READY
FFFC243C 29AE8423		sb		$t0,TCBStatus[$t3]
FFFC2440 289E8D03		ldb		$t0,TCBPriority[$t3]
FFFC2444 19A9000D		insrdy	$a0,$t0
	ret
FFFC2448 00008067
FFFC244C 003D7D13		and		$t0,$t0,#3					; limit to four
FFFC2450 001D1D13		sll		$t0,$t0,#1					; *2 for indexing
FFFC2454 00014637		ldw		$t1,READYQ[$t0]			; get head of queue for that priority
FFFC2458 30860613
FFFC245C 01A60633
FFFC2460 00061D83
FFFC2464 020DD063		bge		$t1,$x0,.insert			; Is there a head?
                        		; level 0 insert - nothing in list, create a ring to self
FFFC2468 00014637		stw		$a0,READYQ[$t0]			; no head, simple to insert
FFFC246C 30860613
FFFC2470 01A60633
FFFC2474 01261023
FFFC2478 2D2E9823		stw		$a0,TCBNext[$t3]		; remember it's a ring
FFFC247C 2D2E9C23		stw		$a0,TCBPrev[$t3]
FFFC2480 00008067		ret
                        		; Insert at tail of list, which is just before the head.
                        	.insert:
FFFC2484 00AD9F13		sll		$t4,$t1,#LOG_TCBSZ	; tid to pointer
FFFC2488 2D8F1E03		ldw		$t2,TCBPrev[$t4]		; t2 = head->prev
FFFC248C 01BE1C63		bne		$t2,$t1,.level2
                        		;level 1 insert - only 1 item currently in list, items point to each other
                        		; This bit probably redundant code as normal insertion should work as well.
FFFC2490 2D2F1C23		stw		$a0,TCBPrev[$t4]		; simple to insert
FFFC2494 2D2F1823		stw		$a0,TCBNext[$t4]		; head->prev = head->next = arg
FFFC2498 2DBE9C23		stw		$t1,TCBPrev[$t3]		; arg->prev = arg->next = head
FFFC249C 2DBE9823		stw		$t1,TCBNext[$t3]
FFFC24A0 00008067		ret
                        		; Here there will be three or more entries in the list. Arg is inserted
                        		; between head and head->prev to make it the last on the list.
                        	.level2:
FFFC24A4 2DCE9C23		stw		$t2,TCBPrev[$t3]		; arg->prev = head->prev
FFFC24A8 2DBE9823		stw		$t1,TCBNext[$t3]		; arg->next = head, arg links are now set
                        		; If there's a head there should be a head->prev and head->next at least pointing to the head
FFFC24AC 000E4663		bltz	$t2,.0001						; head->prev valid?
FFFC24B0 00AE1E93		sll		$t3,$t2,#LOG_TCBSZ	; t3 = head->prev (as a pointer)
FFFC24B4 2D2E9823		stw		$a0,TCBNext[$t3]		; head->prev->next = arg
                        	.0001:
FFFC24B8 2D2F1C23		stw		$a0,TCBPrev[$t4]		; head->prev = arg
FFFC24BC 00008067		ret
                        	.badTid:
FFFC24C0 00106813		ldi		$v0,#E_Arg
FFFC24C4 00008067		ret
                        	
                        	;------------------------------------------------------------------------------
                        	; The ready list is a doubly linked list to make it easier and faster to
                        	; remove items from the list. There's no need to search the list for the item
                        	; to remove.
                        	;
                        	; Parameters:
                        	;		a0 = tid to remove
                        	; Modifies:
                        	;		t0,t1,t2,t3,t4,t5
                        	; Returns:
                        	;		v0 = E_Ok for success
                        	;------------------------------------------------------------------------------
                        	
                        	RemoveFromReadyList:
FFFC24C8 00F06893		ldi		$v1,#MAX_TID					; check arg
FFFC24CC 0128F863		bleu	$a0,$v1,.tidOk
FFFC24D0 00106813		ldi		$v0,#E_Arg
FFFC24D4 00000637		jmp		badTid
FFFC24D8 1E360067
                        	.tidOk:
FFFC24DC 1A09000D		rmvrdy	$a0									; cheat and use hardware
FFFC24E0 00A91F13		sll		$t4,$a0,#LOG_TCBSZ		; s1 = pointer to tcb
FFFC24E4 288F0D03		ldb		$t0,TCBStatus[$t4]		; set status no longer ready or running
FFFC24E8 F7ED7D13		and		$t0,$t0,#~(TS_READY|TS_RUNNING)
	stb		$t0,TCBStatus[$t4]
FFFC24EC 29AF0423
FFFC24F0 00008067		ret
                        	
                        	RemoveFromReadyList2:
FFFC24F4 00F06893		ldi		$v1,#MAX_TID					; check arg
FFFC24F8 0928E063		bgtu	$a0,$v1,.badTid
FFFC24FC 00A91F13		sll		$t4,$a0,#LOG_TCBSZ		; s1 = pointer to tcb
FFFC2500 288F0D03		ldb		$t0,TCBStatus[$t4]		; set status no longer ready or running
FFFC2504 F7ED7D13		and		$t0,$t0,#~(TS_READY|TS_RUNNING)
	stb		$t0,TCBStatus[$t4]
FFFC2508 29AF0423
FFFC250C 289F0D03		ldb		$t0,TCBPriority[$t4]	; t0 = priority
FFFC2510 003D7D13		and		$t0,$t0,#3						; limit to 0-3
FFFC2514 001D1D13		sll		$t0,$t0,#1						; *2 for indexing
FFFC2518 00014637		ldw		$t1,READYQ[$t0]				; get head tid
FFFC251C 30860613
FFFC2520 01A60633
FFFC2524 00061D83
FFFC2528 2D0F1E03		ldw		$t2,TCBNext[$t4]			; get arg->next
FFFC252C 012D9E63		bne		$t1,$a0,.0001					; removing head of list?
FFFC2530 012E1463		bne		$t2,$a0,.0003					; list emptying? (if arg->next points back to arg)
FFFC2534 FFF06E13		ldi		$t2,#-1
                        	.0003:
FFFC2538 00014637		stw		$t2,READYQ[$t0]				; yes, set new head to arg->next
FFFC253C 30860613
FFFC2540 01A60633
FFFC2544 01C61023
                        	.0001:
FFFC2548 000E4E63		bltz	$t2,.0002							; validate t2 (arg->next)
FFFC254C 011E7C63		bgeu	$t2,$v1,.0002					; there should always be an arg->next, arg->prev
FFFC2550 2D8F1E83		ldw		$t3,TCBPrev[$t4]			; because the list is circular t3=arg->prev
FFFC2554 00AE9F93		sll		$t5,$t3,#LOG_TCBSZ		; t5 = arg->prev as a pointer
FFFC2558 2DCF9823		stw		$t2,TCBNext[$t5]			; arg->prev->next = arg->next
FFFC255C 00AE1F93		sll		$t5,$t2,#LOG_TCBSZ		; t5 = arg->next as a pointer
FFFC2560 2DDF9C23		stw		$t3,TCBPrev[$t5]			; arg->next->prev = arg->prev
                        		; Now indicate links in TCB are not in use.
                        	.0002:
FFFC2564 FFF06813		ldi		$v0,#-1
FFFC2568 2D0F1823		stw		$v0,TCBNext[$t4]
FFFC256C 2D0F1C23		stw		$v0,TCBPrev[$t4]
FFFC2570 00006813		ldi		$v0,#E_Ok							; we're ok
FFFC2574 00008067		ret
                        	.badTid:
FFFC2578 00106813		ldi		$v0,#E_Arg
FFFC257C 00008067		ret
                        	
                        	;------------------------------------------------------------------------------
                        	; Parameters:
                        	;		a0 = task id to insert
                        	;		a1 = timeout value
                        	; Modifies:
                        	;		t0,t1,t2,t3,t4,t5
                        	;------------------------------------------------------------------------------
                        	
                        	InsertIntoTimeoutList:
FFFC2580 1139000D		setto	$a0,$a1
	sll		$t5,$a0,#LOG_TCBSZ		; tid to pointer
FFFC2584 00A91F93
FFFC2588 0A000C63		bra		.0003
FFFC258C FFF06D13		ldi		$t0,#-1						
FFFC2590 2DAF9823		stw		$t0,TCBNext[$t5]			; initialize indexes to -1
FFFC2594 2DAF9C23		stw		$t0,TCBPrev[$t5]
FFFC2598 00014637		ldw		$t0,TimeoutList
FFFC259C 32860613
FFFC25A0 00060633
FFFC25A4 00061D03
FFFC25A8 020D5063		bge		$t0,$x0,.0001
                        		; Here the timeout list was empty, easy to insert.
FFFC25AC 2F3FA023		stt		$a1,TCBTimeout[$t5]		; set timeout for task
FFFC25B0 00014637		stw		$a0,TimeoutList				; set head of list
FFFC25B4 32860613
FFFC25B8 00060633
FFFC25BC 01261023
FFFC25C0 00006813		ldi		$v0,#E_Ok							; and return
FFFC25C4 00008067		ret
                        	.0001:
FFFC25C8 FFF06D93		ldi		$t1,#-1								; t1 used as previous index
FFFC25CC 000D6E33		mov		$t2,$t0								; t2 = head of timeout list index
FFFC25D0 00AE1E93		sll		$t3,$t2,#LOG_TCBSZ		; t3 = head of timeout list as a pointer
                        		; Search the timeout list for the proper insertion point. The timeouts are
                        		; relative, so the requested amount is decremented by the amounts in the
                        		; list as it is searched.
                        	.beginWhile:
FFFC25D4 2E0EAF03		ldt		$t4,TCBTimeout[$t3]		; get the timeout
FFFC25D8 033F5663		ble		$a1,$t4,.endWhile			; is it greater than the requested timeout ?
FFFC25DC 41E989B3		sub		$a1,$a1,$t4						; decrement by list amount
FFFC25E0 000EEDB3		mov		$t1,$t3								; t1 = pointer to previous entry
FFFC25E4 2D0E9E83		ldw		$t3,TCBNext[$t3]			; get the next entry in list
FFFC25E8 000EC663		bltz	$t3,.endOfList					; end of list?
FFFC25EC 00AE9E93		sll		$t3,$t3,#LOG_TCBSZ		; no, set pointer to current entry
FFFC25F0 FFBE92E3		bne		$t3,$t1,.beginWhile		; list screwed up? (This test should always be true)
                        	.endOfList:
                        		; Here the end of the list was reached, insert as last entry
FFFC25F4 2D2D9823		stw		$a0,TCBNext[$t1]			; entry->next = arg
FFFC25F8 2DBF9C23		stw		$t1,TCBPrev[$t5]			; arg->prev = entry
FFFC25FC 2F3FA023		stt		$a1,TCBTimeout[$t5]
FFFC2600 04000063		bra		.0003
                        	.endWhile:
                        		; Here we found a timeout greater than the requested amount. Insert before.
FFFC2604 20AEDE13		sra		$t2,$t3,#LOG_TCBSZ		; pointer to tid
FFFC2608 2DCF9823		stw		$t2,TCBNext[$t5]			; arg->next = entry
FFFC260C 20ADDE13		sra		$t2,$t1,#LOG_TCBSZ
FFFC2610 2DCF9C23		stw		$t2,TCBPrev[$t5]			; arg->prev = prev entry
FFFC2614 2E0EAE03		ldt		$t2,TCBTimeout[$t3]		; update timeout of entry by subtracting off
FFFC2618 413E0E33		sub		$t2,$t2,$a1						; the arg's timeout
FFFC261C 2FCEA023		stt		$t2,TCBTimeout[$t3]
FFFC2620 2D2E9C23		stw		$a0,TCBPrev[$t3]			; entry->prev = arg
FFFC2624 000DC663		bltz	$t1,.0002							; previous entry? no - go update list head
FFFC2628 2D2D9823		stw		$a0,TCBNext[$t1]			; prev entry->next = arg
FFFC262C 00000A63		bra		.0003
                        	.0002:
FFFC2630 00014637		stw		$a0,TimeoutList				; list head = arg
FFFC2634 32860613
FFFC2638 00060633
FFFC263C 01261023
                        	.0003:
FFFC2640 288FCE03		ldbu	$t2,TCBStatus[$t5]		; set status to timing out
FFFC2644 010E6E13		or		$t2,$t2,#TS_TIMEOUT
FFFC2648 29CF8423		stb		$t2,TCBStatus[$t5]
FFFC264C 00006813		ldi		$v0,#E_Ok
FFFC2650 00008067		ret
                        	
