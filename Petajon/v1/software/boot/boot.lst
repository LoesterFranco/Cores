                        	;-----------------------------------------------------------------------------
                        	; IO Focus routines complicated by the fact that the base address of TCB
                        	; zero is zero (looks like a null pointer but isn't). So the value -1 is 
                        	; used to indicate no focus index.
                        	;-----------------------------------------------------------------------------
                        	
                        	.file "../fmtk/iofocus.r5a",34
                        		code
                        		align	4                        	
                        	
                        	;-----------------------------------------------------------------------------
                        	; Determine if the currently running task has the I/O focus.
                        	;
                        	; Stack Space:
                        	;		2 words
                        	; Parameters:
                        	;		none
                        	; Modifies:
                        	;		none
                        	; Returns:
                        	;		v0 = E_Ok
                        	;		v1 = 1 if task has IO focus, 0 otherwise
                        	;-----------------------------------------------------------------------------
                        	
                        	FMTK_HasIOFocus:
                        		
FFFC29D4 18101873		csrrw	v0,#$181,x0
FFFC29D8 00F87813		and		v0,v0,#15
FFFC29DC 00A81813	sll		$v0,$v0,#LOG_TCBSZ
FFFC29E0 00009637		ldt		$v1,IOFocusNdx
FFFC29E4 C0860613
FFFC29E8 00060633
FFFC29EC 00062883
FFFC29F0 01180663		beq		$v0,$v1,.hasFocus
FFFC29F4 00006893		ldi		$v1,#0
FFFC29F8 00000463		bra		.xit
                        	.hasFocus:
FFFC29FC 00106893		ldi		$v1,#1
                        	.xit:
FFFC2A00 00006813		ldi		$v0,#E_Ok
FFFC2A04 E30FF06F		jmp		ERETx
                        	
                        	;-----------------------------------------------------------------------------
                        	; First check if it's even possible to switch the focus to another
                        	; task. The I/O focus list could be empty or there may be only a
                        	; single task in the list. In either case it's not possible to
                        	; switch.
                        	;
                        	;	Parameters:
                        	;		none
                        	;	Modifies:
                        	;		none
                        	;	Returns:
                        	;		v0 = E_Ok
                        	;-----------------------------------------------------------------------------
                        	
                        	SwitchIOFocusHelper:
FFFC2A08 00009637		ldt		$t0,IOFocusNdx			; get focus pointer
FFFC2A0C C0860613
FFFC2A10 00060633
FFFC2A14 00062D03
FFFC2A18 020D4063		bltz	$t0,.noFocus		; is it -1?
FFFC2A1C 2E8D2D83		ldt		$t1,IOF_NEXT[$t0]
FFFC2A20 01AD8C63		beq		$t1,$t0,.sameFocus
FFFC2A24 000DCA63		bltz	$t1,.noFocus
                        		; swap virtual screens
                        		; set vidmem pointer
FFFC2A28 00009637		stt		$t1,IOFocusNdx
FFFC2A2C C0860613
FFFC2A30 00060633
FFFC2A34 01B62023
                        	.sameFocus:
                        	.noFocus:
FFFC2A38 00006813		ldi		$v0,E_Ok
FFFC2A3C 00008067		ret
                        	
                        	FMTK_SwitchIOFocus:
FFFC2A40 FC9FF0EF		call	SwitchIOFocusHelper
FFFC2A44 DF0FF06F		jmp		ERETx
                        	
                        	;-----------------------------------------------------------------------------
                        	; The I/O focus list is an array indicating which jobs are requesting the
                        	; I/O focus. The I/O focus is user controlled by pressing CNTRL-T on the
                        	; keyboard.
                        	;
                        	; Parameters:
                        	;		a1 = task id requesting focus for
                        	;-----------------------------------------------------------------------------
                        	
                        	FMTK_RequestIOFocus:
FFFC2A48 00106D13		ldi		$t0,#1
FFFC2A4C 013D1D33		sll		$t0,$t0,$a1
FFFC2A50 00009637		ldt		$t1,IOFocusTbl			; Is the task already included in the IO focus?
FFFC2A54 C1060613
FFFC2A58 00060633
FFFC2A5C 00062D83
FFFC2A60 01ADFE33		and		$t2,$t1,$t0					; test bit
FFFC2A64 060E1263		bne		$t2,$x0,.ret				; If so, don't add again
FFFC2A68 01ADEDB3		or		$t1,$t1,$t0					; set bit indicator
FFFC2A6C 00009637		stt		$t1,IOFocusTbl
FFFC2A70 C1060613
FFFC2A74 00060633
FFFC2A78 01B62023
FFFC2A7C 00009637		ldt		$t0,IOFocusNdx			; get current index
FFFC2A80 C0860613
FFFC2A84 00060633
FFFC2A88 00062D03
FFFC2A8C 00A99D93		sll		$t1,$a1,#LOG_TCBSZ	; t1 = pointer to TCB
FFFC2A90 020D5063		bge		$t0,$x0,.notEmpty		; is there one? (!= -1)
FFFC2A94 00009637		stt		$t1,IOFocusNdx			; no current index, so set equal to requester
FFFC2A98 C0860613
FFFC2A9C 00060633
FFFC2AA0 01B62023
FFFC2AA4 2FBDA423		stt		$t1,IOF_NEXT[$t1]		; and loop back to self
FFFC2AA8 2FBDA823		stt		$t1,IOF_PREV[$t1]
FFFC2AAC 00000E63		bra		.ret
                        	.notEmpty:
FFFC2AB0 2F0D2E03		ldt		$t2,IOF_PREV[$t0]		; insert t1 into focus ring
FFFC2AB4 2FCDA823		stt		$t2,IOF_PREV[$t1]
FFFC2AB8 2FADA423		stt		$t0,IOF_NEXT[$t1]
FFFC2ABC 2F0D2E03		ldt		$t2,IOF_PREV[$t0]
FFFC2AC0 2FBE2423		stt		$t1,IOF_NEXT[$t2]
FFFC2AC4 2FBD2823		stt		$t1,IOF_PREV[$t0]
                        	.ret:
FFFC2AC8 00006813		ldi		$v0,#E_Ok
FFFC2ACC D68FF06F		jmp		ERETx
                        	
                        	;-----------------------------------------------------------------------------
                        	; ReleaseIOFocus called when the task no longer desires to be on the I/O
                        	; focus list.
                        	;-----------------------------------------------------------------------------
                        	
                        	FMTK_ReleaseIOFocus:
                        		
FFFC2AD0 18101873		csrrw	v0,#$181,x0
FFFC2AD4 00F87813		and		v0,v0,#15
FFFC2AD8 000869B3	mov		a1,v0
                        		; fall into ForceReleaseIOFocus
                        	
                        	;-----------------------------------------------------------------------------
                        	; Releasing the I/O focus causes the focus to switch if the running job
                        	; had the I/O focus.
                        	; ForceReleaseIOFocus forces the release of the IO focus for a job
                        	; different than the one currently running.
                        	; 
                        	; Stack Space:
                        	;		2 words
                        	; Parameters:
                        	;		a1 = task id to release
                        	; Returns:
                        	;		v0 = E_Ok
                        	;-----------------------------------------------------------------------------
                        	
                        	FMTK_ForceReleaseIOFocus:
FFFC2ADC 00106D13		ldi		$t0,#1
FFFC2AE0 013D1D33		sll		$t0,$t0,$a1
FFFC2AE4 00009637		ldt		$t1,IOFocusTbl
FFFC2AE8 C1060613
FFFC2AEC 00060633
FFFC2AF0 00062D83
FFFC2AF4 01ADFE33		and		$t2,$t1,$t0				; test bit for task
FFFC2AF8 080E0663		beq		$t2,$x0,.noFocus	; does it even have the focus?
FFFC2AFC FFFD4D13		xor		$t0,$t0,#-1				; get inverted mask
FFFC2B00 01ADFDB3		and		$t1,$t1,$t0				; clear bit for task
FFFC2B04 00009637		stt		$t1,IOFocusTbl
FFFC2B08 C1060613
FFFC2B0C 00060633
FFFC2B10 01B62023
FFFC2B14 00009637		ldt		$t1,IOFocusNdx		; check if the focus being released is the current
FFFC2B18 C0860613
FFFC2B1C 00060633
FFFC2B20 00062D83
FFFC2B24 00A99D13		sll		$t0,$a1,#LOG_TCBSZ	; io focus. If so, switch focus
FFFC2B28 03BD1063		bne		$t0,$t1,.notSame
FFFC2B2C FF870713		sub		$sp,$sp,#8
FFFC2B30 01A72023		stt		$t0,[$sp]
FFFC2B34 01B72223		stt		$t1,4[$sp]
FFFC2B38 ED1FF0EF		call	SwitchIOFocusHelper
FFFC2B3C 00072D03		ldt		$t0,[$sp]
FFFC2B40 00472D83		ldt		$t1,4[$sp]
FFFC2B44 00870713		add		$sp,$sp,#8
                        	.notSame:
FFFC2B48 2E8D2E03		ldt		$t2,IOF_NEXT[$t0]
FFFC2B4C 020E4C63		blt		$t2,$x0,.done
FFFC2B50 01AE0A63		beq		$t2,$t0,.pjSame
FFFC2B54 2F0D2D83		ldt		$t1,IOF_PREV[$t0]
FFFC2B58 2FBE2823		stt		$t1,IOF_PREV[$t2]
FFFC2B5C 2FCDA423		stt		$t2,IOF_NEXT[$t1]
FFFC2B60 00000C63		bra		.0001
                        	.pjSame:
FFFC2B64 FFF06D93		ldi		$t1,#-1
FFFC2B68 00009637		stt		$t1,IOFocusNdx
FFFC2B6C C0860613
FFFC2B70 00060633
FFFC2B74 01B62023
                        	.0001:
FFFC2B78 FFF06D93		ldi		$t1,#-1
FFFC2B7C 2FBD2423		stt		$t1,IOF_NEXT[$t0]	
FFFC2B80 2FBD2823		stt		$t1,IOF_PREV[$t0]	
                        	.done:
                        	.noFocus:
FFFC2B84 00006813		ldi		$v0,#E_Ok
FFFC2B88 CACFF06F		jmp		ERETx
