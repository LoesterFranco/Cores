FFFC42E0 A29FE0EF		call	GOOUT
FFFC42E4 02006913		ldi		a0,#' '
FFFC42E8 A21FE0EF		call	GOOUT
FFFC42EC 71C06D93		ldi		$t1,#BUFFER
FFFC42F0 F9CDDAE3		ble		$t2,$t1,.GL1	; any char.'s left?	; if not
